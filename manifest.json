{
   "name": "MERA $tudy",
   "description" : "Helps students manage their study sessions",
   "version": "1.0",
   "manifest_version": 2,
   "options_page": "pages/options.html",
   "browser_action": {
     "default_title": "MERA $tudy",
     "default_popup": "pages/popup.html"
   },
   "icons": {
     "128": "img/icon.png"
   },
   "background": {
     "scripts": ["js/eventPage.js"],
     "persistent": false
   },
   "permissions": ["tabs"]
 }
